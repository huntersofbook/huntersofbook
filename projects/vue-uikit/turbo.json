{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
        "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": [
        "build",
        "lint"
      ],
      "outputs": []
    }
  }
}
