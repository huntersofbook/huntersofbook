{
  "name": "@huntersofbook/vue-uikit",
  "type": "module",
  "version": "0.1.0",
  "description": "Infinite loading component for Vue",
  "author": "Mehmet - productdevbook <hi@productdevbook.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/productdevbook",
  "homepage": "https://github.com/huntersofbook/huntersofbook/tree/main/projects/vue-uikit/packages/vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huntersofbook/huntersofbook.git",
    "directory": "projects/vue-uikit/packages/vue"
  },
  "bugs": {
    "url": "https://github.com/huntersofbook/huntersofbook/issues"
  },
  "keywords": [
    "vue 3",
    "vue",
    "uikit"
  ],
  "exports": {
    ".": {
      "types": "./dist/lib/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tailwindcss": {
      "import": "./dist/tailwindcss/index.mjs",
      "require": "./dist/tailwindcss/index.cjs",
      "types": "./dist/tailwindcss/index.d.ts"
    },
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "import": "./dist/plugin/index.js"
    },
    "./plugin/vite": {
      "types": "./dist/plugin/vite.d.ts",
      "import": "./dist/plugin/vite.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/lib/types/index.d.ts",
  "files": [
    "dist",
    "volar.d.ts",
    "tailwind.d.ts",
    "tailwind.js"
  ],
  "scripts": {
    "build": "vite build && pnpm plugin:build && pnpm build:tailwindcss",
    "build:tailwindcss": "pnpm tsup --config ./tailwindcss/tsup.config.ts",
    "dev": "vite build --watch",
    "plugin:build": "pnpm tsup",
    "dev:plugin": "pnpm tsup --watch",
    "prepublishOnly": "npm run build",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "vue": "3"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    },
    "@vueuse/core": {
      "optional": false
    }
  },
  "dependencies": {
    "@antfu/utils": "^0.7.2",
    "@rollup/pluginutils": "^5.0.2",
    "@types/minimatch": "^5.1.2",
    "chokidar": "^3.5.3",
    "debug": "^4.3.4",
    "defu": "^6.1.1",
    "filter-anything": "^3.0.5",
    "globby": "^13.1.3",
    "minimatch": "^5.1.2",
    "unplugin": "^1.0.1",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.11.18",
    "@types/postcss-js": "^4.0.0",
    "@types/rollup-plugin-css-only": "^3.1.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vueuse/core": "^9.9.0",
    "eslint": "^8.28.0",
    "esno": "^0.16.3",
    "pathe": "^1.0.0",
    "postcss-js": "^4.0.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "unconfig": "^0.3.7",
    "unplugin-auto-import": "^0.12.1",
    "unplugin-vue-define-options": "^1.1.1",
    "unplugin-vue-macros": "^1.1.1",
    "vite": "^4.0.3",
    "vite-plugin-dts": "^1.7.1",
    "vite-plugin-static-copy": "^0.13.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.2.45"
  }
}