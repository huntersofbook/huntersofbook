{
  "name": "huntersofbook",
  "version": "0.3.2",
  "license": "MIT",
  "description": "huntersofbook is an initiative based on books. This library contains open source tools.",
  "author": "Mehmet - productdevbook <hi@productdevbook.com>",
  "funding": "https://github.com/sponsors/productdevbook",
  "homepage": "https://github.com/huntersofbook/huntersofbook#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huntersofbook/huntersofbook.git"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/productdevbook"
  },
  "bugs": {
    "url": "https://github.com/huntersofbook/huntersofbook/issues"
  },
  "keywords": [
    "huntersofbook",
    "css",
    "tailwind",
    "vue"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "require": "./dist/ui.cjs",
      "import": "./dist/ui.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@huntersofbook/core": "workspace:*",
    "@huntersofbook/ui": "workspace:*"
  }
}
