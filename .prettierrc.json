

{
    "$schema": "http://json.schemastore.org/prettierrc",
    "printWidth": 80,
    "semi": false,
    "useTabs": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "requirePragma": true,
    "insertPragma": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "css",
    "vueIndentScriptAndStyle": false,
    "embeddedLanguageFormatting": "auto",
    "endOfLine": "lf",
    "tabWidth": 2
  }